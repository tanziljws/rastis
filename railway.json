{
  "build": {
    "builder": "docker"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": null,
    "restartPolicyType": "ON_FAILURE",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100
  },
  "envConfig": {
    "PORT": "8080",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "${RAILWAY_PUBLIC_DOMAIN}",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "file",
    "QUEUE_CONNECTION": "sync",
    "FILESYSTEM_DISK": "public"
  }
}




